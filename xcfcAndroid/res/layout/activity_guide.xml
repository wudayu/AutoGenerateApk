<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res/com.movitech.grande.haerbin"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <com.movitech.grande.views.BaseViewPager
        android:id="@+id/vp_guide"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <View
        android:id="@+id/puppet_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.movitech.grande.views.CirclePageIndicator
        android:id="@+id/indicator"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/puppet_view"
        android:layout_marginBottom="@dimen/dp_fifty"
        app:pageColor="@color/letter_green_deep_darker"
        app:strokeWidth="0dp"
        android:visibility="gone" />
</RelativeLayout>