<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/main_app" >
    <RelativeLayout
        android:id="@+id/rl_personal_info"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_rl_personal_info_fragment_mine"
        android:background="@drawable/my_toptit_bg" >
        <RelativeLayout
            android:id="@+id/rl_user_image"
            android:layout_width="@dimen/width_rl_user_image_fragment_mine"
            android:layout_height="@dimen/height_rl_user_image_fragment_mine"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_ten"
            android:background="@drawable/userimg_box" >
            <ImageView
                android:id="@+id/iv_user_image"
                android:layout_width="@dimen/side_iv_user_image_fragment_mine"
                android:layout_height="@dimen/side_iv_user_image_fragment_mine"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/dp_eight"
                android:contentDescription="@null" />
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/rl_name_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/margin_right_rl_name_detail_fragment_mine"
            android:layout_marginTop="@dimen/margin_top_rl_name_detail_fragment_mine"
            android:layout_toLeftOf="@id/rl_user_image"
            android:orientation="vertical" >
            <LinearLayout
                android:id="@+id/ll_certification_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_five"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <ImageView
                    android:id="@+id/iv_certification_status"
                    android:layout_width="@dimen/side_iv_rl_name_detail_fragment_mine"
                    android:layout_height="@dimen/side_iv_rl_name_detail_fragment_mine"
                    android:background="@drawable/ico_certification_ok"
                    android:contentDescription="@null" />
                <TextView
                    android:id="@+id/txt_certification_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_five"
                    android:textColor="@color/col_words_active_white"
                    android:textSize="@dimen/px24" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/ll_level_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_five"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
                <ImageView
                    android:layout_width="@dimen/side_iv_rl_name_detail_fragment_mine"
                    android:layout_height="@dimen/side_iv_rl_name_detail_fragment_mine"
                    android:background="@drawable/ico_zuan"
                    android:contentDescription="@null" />
                <TextView
                    android:id="@+id/txt_level_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_five"
                    android:textColor="@color/col_words_active_white"
                    android:textSize="@dimen/px24" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_current_place"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_left_rl_current_place_fragment_mine"
            android:layout_marginTop="@dimen/margin_top_rl_current_place_fragment_mine"
            android:layout_toRightOf="@id/rl_user_image"
            android:orientation="vertical" >
            <TextView
                android:id="@+id/txt_current_position"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:textColor="@color/col_words_active_white"
                android:textSize="@dimen/px36"
                android:textStyle="bold" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_five"
                android:text="@string/txt_hint_current_position"
                android:textColor="@color/col_words_active_white"
                android:textSize="@dimen/px24" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/rl_commission_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_rl_commission_info_fragment_mine"
            android:layout_alignParentBottom="true"
            android:background="@color/transparent_black_30_100"
            android:baselineAligned="false"
            android:orientation="horizontal" >
            <RelativeLayout
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/transparent_black_50_100" >
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="vertical" >
                    <TextView
                        android:id="@+id/txt_commission_total"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px36" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/dp_five"
                        android:text="@string/txt_hint_commission_total"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px22" />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="3"
                android:paddingLeft="@dimen/dp_twenty"
                android:paddingRight="@dimen/dp_twenty" >
                <LinearLayout
                    android:id="@+id/ll_wait_commission"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:orientation="vertical" >
                    <TextView
                        android:id="@+id/txt_wait_commission"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px36" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/dp_five"
                        android:text="@string/txt_hint_wait_commission"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px22" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_grant_commission"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:orientation="vertical" >
                    <TextView
                        android:id="@+id/txt_grant_commission"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px36" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/dp_five"
                        android:text="@string/txt_hint_grant_commission"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px22" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_earned_commission"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:orientation="vertical" >
                    <TextView
                        android:id="@+id/txt_earned_commission"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px36" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="@dimen/dp_five"
                        android:text="@string/txt_hint_earned_commission"
                        android:textColor="@color/col_words_active_white"
                        android:textSize="@dimen/px22" />
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toLeftOf="@id/ll_grant_commission"
                    android:layout_toRightOf="@id/ll_wait_commission" >
                    <ImageView
                        android:layout_width="@dimen/side_icon_plus_fragment_mine"
                        android:layout_height="@dimen/side_icon_plus_fragment_mine"
                        android:layout_centerInParent="true"
                        android:background="@drawable/ico_plus"
                        android:contentDescription="@null" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_toLeftOf="@id/ll_earned_commission"
                    android:layout_toRightOf="@id/ll_grant_commission" >
                    <ImageView
                        android:layout_width="@dimen/side_icon_plus_fragment_mine"
                        android:layout_height="@dimen/side_icon_plus_fragment_mine"
                        android:layout_centerInParent="true"
                        android:background="@drawable/ico_plus"
                        android:contentDescription="@null" />
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@id/rl_commission_info"
            android:layout_below="@id/rl_user_image" >
            <TextView
                android:id="@+id/txt_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:textColor="@color/col_words_active_white"
                android:textSize="@dimen/px36" />
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_client_block_fragment_mine"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_client_block_fragment_mine"
        android:layout_below="@id/rl_personal_info"
        android:layout_margin="@dimen/dp_five"
        android:paddingLeft="@dimen/padding_left_right_block_fragment_mine"
        android:paddingRight="@dimen/padding_left_right_block_fragment_mine" >
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_two"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/dp_thirty"
            android:layout_marginRight="@dimen/dp_thirty"
            android:background="@color/col_text_recommend_yellow_outter" />
        <LinearLayout
            android:id="@+id/ll_client_block_fragment_mine"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:baselineAligned="false"
            android:orientation="horizontal" >
            <RelativeLayout
                android:id="@+id/ll_recommend_confirmed"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" >
                <RelativeLayout
                    android:layout_width="@dimen/side_small_circle_fragment_mine"
                    android:layout_height="@dimen/side_small_circle_fragment_mine"
                    android:layout_centerInParent="true"
                    android:background="@drawable/shape_small_circle_fragment_mine" >
                    <TextView
                        android:id="@+id/txt_count_recommend_confirmed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:textColor="@color/letter_grey_deep_full"
                        android:textSize="@dimen/px36" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="@string/txt_hint_count_recommend_confirmed"
                    android:textColor="@color/col_text_recommend_yellow_outter"
                    android:textSize="@dimen/px22" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ll_recommend_successed"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" >
                <RelativeLayout
                    android:layout_width="@dimen/side_small_circle_fragment_mine"
                    android:layout_height="@dimen/side_small_circle_fragment_mine"
                    android:layout_centerInParent="true"
                    android:background="@drawable/shape_small_circle_fragment_mine" >
                    <TextView
                        android:id="@+id/txt_count_recommend_successed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/dp_two"
                        android:textColor="@color/letter_grey_deep_full"
                        android:textSize="@dimen/px34" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="@string/txt_hint_count_recommend_successed"
                    android:textColor="@color/col_text_recommend_yellow_outter"
                    android:textSize="@dimen/px22" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ll_importent_client"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" >
                <RelativeLayout
                    android:layout_width="@dimen/side_big_circle_fragment_mine"
                    android:layout_height="@dimen/side_big_circle_fragment_mine"
                    android:layout_centerInParent="true"
                    android:background="@drawable/shape_big_circle_fragment_mine" >
                    <TextView
                        android:id="@+id/txt_count_importent_client"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/dp_two"
                        android:textColor="@color/letter_grey_deep_full"
                        android:textSize="@dimen/px36" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="@string/txt_hint_count_importent_client"
                    android:textColor="@color/letter_red_deep_standard"
                    android:textSize="@dimen/px22" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ll_sea_house"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" >
                <RelativeLayout
                    android:layout_width="@dimen/side_small_circle_fragment_mine"
                    android:layout_height="@dimen/side_small_circle_fragment_mine"
                    android:layout_centerInParent="true"
                    android:background="@drawable/shape_small_circle_fragment_mine" >
                    <TextView
                        android:id="@+id/txt_count_sea_house"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/dp_two"
                        android:textColor="@color/letter_grey_deep_full"
                        android:textSize="@dimen/px36" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="@string/txt_hint_count_visited_houses"
                    android:textColor="@color/col_text_recommend_yellow_outter"
                    android:textSize="@dimen/px22" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/ll_sell_successed"
                android:layout_width="0dip"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical" >
                <RelativeLayout
                    android:layout_width="@dimen/side_small_circle_fragment_mine"
                    android:layout_height="@dimen/side_small_circle_fragment_mine"
                    android:layout_centerInParent="true"
                    android:background="@drawable/shape_small_circle_fragment_mine" >
                    <TextView
                        android:id="@+id/txt_count_sell_successed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="@dimen/dp_two"
                        android:textColor="@color/letter_grey_deep_full"
                        android:textSize="@dimen/px36" />
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_centerHorizontal="true"
                    android:text="@string/txt_hint_count_sell_successed"
                    android:textColor="@color/col_text_recommend_yellow_outter"
                    android:textSize="@dimen/px22" />
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_my_score"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_rl_my_score_fragment_mine"
        android:layout_below="@id/rl_client_block_fragment_mine"
        android:layout_marginBottom="@dimen/dp_five"
        android:layout_marginLeft="@dimen/dp_five"
        android:layout_marginRight="@dimen/dp_five"
        android:layout_marginTop="@dimen/dp_twenty"
        android:background="@drawable/shape_block_fragment_mine"
        android:paddingLeft="@dimen/padding_left_right_block_fragment_mine"
        android:paddingRight="@dimen/padding_left_right_block_fragment_mine" >
        <ImageView
            android:id="@+id/iv_my_score"
            android:layout_width="@dimen/side_iv_my_score_attention_fragment_mine"
            android:layout_height="@dimen/side_iv_my_score_attention_fragment_mine"
            android:layout_centerVertical="true"
            android:background="@drawable/ico_integral"
            android:contentDescription="@null" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_eight"
            android:layout_toRightOf="@id/iv_my_score"
            android:text="@string/txt_hint_my_score"
            android:textColor="@color/letter_main_red"
            android:textSize="@dimen/px34" />
        <ImageView
            android:id="@+id/iv_my_score_indicator"
            android:layout_width="@dimen/width_my_score_fragment_mine"
            android:layout_height="@dimen/height_my_score_fragment_mine"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/ico_readmore"
            android:contentDescription="@null" />
        <TextView
            android:id="@+id/txt_integral"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_ten"
            android:layout_toLeftOf="@id/iv_my_score_indicator"
            android:textColor="@color/col_txt_my_score"
            android:textSize="@dimen/px24" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_block"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_fifteen"
        android:layout_below="@id/rl_my_score"
        android:visibility="gone" >
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_my_attention"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_rl_my_attention_fragment_mine"
        android:layout_below="@id/rl_block"
        android:layout_margin="@dimen/dp_five"
        android:background="@drawable/shape_block_fragment_mine"
        android:paddingLeft="@dimen/padding_left_right_block_fragment_mine"
        android:paddingRight="@dimen/padding_left_right_block_fragment_mine" >
        <ImageView
            android:id="@+id/iv_my_attention"
            android:layout_width="@dimen/side_iv_my_score_attention_fragment_mine"
            android:layout_height="@dimen/side_iv_my_score_attention_fragment_mine"
            android:layout_centerVertical="true"
            android:background="@drawable/ico_fav"
            android:contentDescription="@null" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_eight"
            android:layout_toRightOf="@id/iv_my_attention"
            android:text="@string/txt_hint_my_attention"
            android:textColor="@color/letter_main_red"
            android:textSize="@dimen/px34" />
        <ImageView
            android:layout_width="@dimen/width_my_score_fragment_mine"
            android:layout_height="@dimen/height_my_score_fragment_mine"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/ico_readmore"
            android:contentDescription="@null" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_my_team"
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_rl_my_score_fragment_mine"
        android:layout_below="@id/rl_my_attention"
        android:layout_marginBottom="@dimen/dp_five"
        android:layout_marginLeft="@dimen/dp_five"
        android:layout_marginRight="@dimen/dp_five"
        android:layout_marginTop="@dimen/dp_five"
        android:background="@drawable/shape_block_fragment_mine"
        android:paddingLeft="@dimen/padding_left_right_block_fragment_mine"
        android:paddingRight="@dimen/padding_left_right_block_fragment_mine"
        android:visibility="gone" >
        <ImageView
            android:id="@+id/iv_my_team"
            android:layout_width="@dimen/side_iv_my_score_attention_fragment_mine"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/ico_mine_team"
            android:contentDescription="@null" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_eight"
            android:layout_toRightOf="@id/iv_my_team"
            android:text="@string/txt_mine_team_tag"
            android:textColor="@color/letter_green_deep_standard"
            android:textSize="@dimen/px34" />
        <ImageView
            android:id="@+id/iv_my_team_indicator"
            android:layout_width="@dimen/width_my_score_fragment_mine"
            android:layout_height="@dimen/height_my_score_fragment_mine"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/ico_readmore"
            android:contentDescription="@null" />
        <TextView
            android:id="@+id/txt_my_team"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/dp_ten"
            android:layout_toLeftOf="@id/iv_my_team_indicator"
            android:textColor="@color/col_txt_my_score"
            android:textSize="@dimen/px24" />
    </RelativeLayout>
</RelativeLayout>